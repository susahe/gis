#This is the Model File of GS
from django.db import models

class GramaSevaDivision(models.Model):
        gs_division = models.CharField(max_length=300)
        gs_village  = models.CharField(max_length=300)
        gs_road     =  models.CharField(max_length=300)

class person(models.Model):
         p_fname = models.CharField(max_length=100)
         p_lname = models.CharField(max_length=300)
         p_oname =  models.CharField(max_length=300)
         p_nic  = models.CharField(max_length=12)
         p_type = models.CharField(max_length=20)
                 
            
